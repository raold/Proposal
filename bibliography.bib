@article{Moorman1994A-model-for-str,
	Abstract = {Clinical narratives in patient records are usually recorded in free text, limiting the use of this information for research, quality assessment, and decision support. This study focuses on the capture of clinical narratives in a structured format by supporting physicians with structured data entry (SDE). We analyzed and made explicit which requirements SDE should meet
to be acceptable for the physician on the one hand, and generate unambig- uous patient data on the other. Starting from these requirements, we found that in order to support SDE, the knowledge on which it is based needs to be '1 made explicit: we refer to this knowledge as descriptional knowledge. We articulate the nature of this knowledge, and propose a model in which it can be formally represented. The model allows the construction of specific knowledge bases, each representing the knowledge needed to support SDE within a circumscribed domain. Data entry is made possible through a gen- eral entry program, of which the behavior is determined by a combination of user input and the content of the applicable domain knowledge base. We clarify how descriptional knowledge is represented, modeled, and used for data entry to achieve SDE, which meets the proposed requirements.},
	Author = {Moorman, PW and Van Ginneken, AM and van der Lei, J and van Bemmel, JH},
	Date-Added = {2012-02-07 23:25:20 +0000},
	Date-Modified = {2012-02-08 14:32:59 +0000},
	Journal = {Methods Inform Med},
	Keywords = {data entry},
	Number = {5},
	Pages = {454-63},
	Title = {A model for structured data entry based on explicit descriptional knowledge},
	Volume = {33},
	Year = {1994},
	Bdsk-File-1 = {}}

