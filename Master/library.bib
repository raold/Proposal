keywords = {Artificial Intelligence,Computer Systems,Database Management Systems,Humans,Information Storage and Retrieval,Medical Records Systems, Computerized,Medical Records Systems, Computerized: organizatio,Semantics,User-Computer Interface,Vocabulary, Controlled},
month = jan,
