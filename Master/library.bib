keywords = {Clinical Competence,Communication,Competency-Based Education,Curriculum,Dental Care,Dental Caries,Dental Caries Susceptibility,Dental Caries: diagnosis,Dental Caries: prevention \& control,Dental Caries: therapy,Dental Records,Dental Research,Education, Dental,Educational Measurement,Evidence-Based Medicine,Evidence-Based Medicine: education,Faculty, Dental,Forms and Records Control,Humans,Indiana,Information Dissemination,Medical Records Systems, Computerized,Outcome Assessment (Health Care),Problem-Based Learning,Risk Assessment,Schools, Dental,Schools, Dental: organization \& administration,Teaching,Teaching: methods,Thinking},
month = may,
